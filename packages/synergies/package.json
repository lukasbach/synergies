{
  "name": "synergies",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "git@github.com:lukasbach/synergies.git",
    "directory": "packages/synergies"
  },
  "sideEffects": false,
  "author": "Lukas Bach",
  "license": "MIT",
  "private": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "esnext": "lib/esnext/index.js",
  "typings": "lib/esm/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:all": "run-p \"build:cjs\" \"build:esm\" \"build:esnext\" \"build:types\"",
    "build:cjs": "tsc -m commonjs --outDir lib/cjs",
    "build:types": "tsc --declaration true --emitDeclarationOnly --outDir lib/types",
    "build:esm": "tsc -m es2015 --outDir lib/esm",
    "build:esnext": "tsc -m esnext --outDir lib/esnext",
    "start": "tsc -w",
    "test": "jest",
    "prepublish": "yarn build:all && yarn test && cpy \"../../README.md\" ."
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^24.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.0",
    "babel-jest": "^26.6.3",
    "cpy-cli": "^3.1.1",
    "jest": "^26.6.3",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "immer": "*",
    "react": "*",
    "react-dom": "*"
  }
}
